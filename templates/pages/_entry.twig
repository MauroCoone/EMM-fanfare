<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8" />
    <title>{{entry.title}}</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=5.0, viewport-fit=cover" />
    <meta name="referrer" content="origin-when-cross-origin" />
    <meta name="description" content="{{descriptionwebsite.descriptionwebsite | raw}}">
    <link rel="icon" href="{{logo.logo.one().url}}" type="image/x-icon">

    <link rel="stylesheet" href="/assets/css/main.css">
    <script src="https://cdn.tailwindcss.com"></script>
    <script crossorigin="anonymous"
        src="https://polyfill.io/v3/polyfill.min.js?flags=gated&features=Object.assign%2CIntersectionObserver"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/lozad/dist/lozad.min.js"></script>
    <script src="/assets/js/gsap.min.js"></script>  
    <script src="/assets/js/ScrollTrigger.min.js"></script>
</head>
<body>
    {% include 'partials/navigation.twig' %}
        {% if entry.slug == 'galerij' %}
            <div class="mt-[5rem]">
                {% include 'components/pages/components/header'%}
            </div>
            {{ sprig('components/pages/gallery', {'limit': 6}) }}
        {% endif %}
        {% if entry.slug == 'evenementen' %}
            {% include 'components/pages/evenementen'%}
        {% endif %}
        {% if entry.slug == 'orkest' %}
            {% include 'components/pages/orkest'%}
        {% endif %}
        {% if entry.slug == 'geschiedenis' %}
            {% include 'components/pages/geschiedenis'%}
        {% endif %}
        {% if entry.slug == 'contact' %}
            {% include 'components/pages/contact'%}
        {% endif %}
    {% include 'partials/footer.twig' %}

    <script src="/assets/js/animations/fade-in.js"></script>
    <script src="/assets/js/animations/fade-in-left.js"></script>
    <script src="/assets/js/animations/fade-in-right.js"></script>
    <script src="/assets/js/animations/horizontal.js"></script>
</body>
<script src="/assets/js/main.js"></script>
<script src="/assets/js/lozad.js"></script>
<script src="/assets/js/navigation.js"></script>
</html>
